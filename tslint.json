{
  "extends": ["tslint:recommended", "tslint-react"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/registerServiceWorker.ts",
      "(^((?!.*(src/)).).*(spec|test)\\.ts.*$)"
    ]
  },
  "rules": {
    "ordered-imports": false
  }
}
